<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/p3"
    android:translationY="2dp"
    tools:context=".trivia2">

    <TextView
        android:id="@+id/font2"
        android:layout_width="243dp"
        android:layout_height="81dp"
        android:layout_marginStart="90dp"
        android:layout_marginTop="236dp"
        android:fontFamily="casual"
        android:text="Quick Question"
        android:textAppearance="@style/TextAppearance.AppCompat.Body1"
        android:textColor="#100A0A"
        android:textSize="30sp"
        android:textStyle="bold|italic"
        android:typeface="sans"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="328dp"
        android:layout_height="97dp"
        android:layout_marginLeft="-290dp"
        android:layout_marginTop="300dp"
        android:fontFamily="casual"
        android:text="How Would You Describe Your Cooking Skills?"
        android:textAlignment="center"
        android:textAllCaps="false"
        android:textColor="#000000"
        android:textSize="24sp"
        android:textStyle="bold"
        android:translationX="-5dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.494"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/font2"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/button3"
        android:layout_width="141dp"
        android:layout_height="124dp"
        android:layout_marginLeft="-350dp"
        android:layout_marginTop="450dp"
        android:background="@drawable/rounded_button"
        android:backgroundTint="#8DFDF3FF"
        android:onClick="choice"
        android:textColor="#000000"
        android:tooltipText="beginner"
        android:translationY="10dp"
        app:backgroundTint="#5CFFFEFD"
        app:iconTint="#000000"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/button4"
        app:layout_constraintHorizontal_bias="0.427"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView"
        app:layout_constraintVertical_bias="0.034"
        tools:ignore="UsingOnClickInXml,SpeakableTextPresentCheck" />

    <Button
        android:id="@+id/button4"
        android:layout_width="147dp"
        android:layout_height="126dp"
        android:layout_marginStart="50dp"
        android:layout_marginTop="450dp"
        android:background="@drawable/rounded_button"
        android:backgroundTint="#FFFFFF"
        android:onClick="choice"
        android:textAlignment="viewStart"
        android:textColor="#000000"
        android:textSize="14sp"
        android:tooltipText="intermediate"
        android:translationX="25dp"
        android:translationY="10dp"
        app:backgroundTint="#5CFFFEFD"
        app:iconTint="#000000"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.82"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView"
        app:layout_constraintVertical_bias="0.033"
        tools:ignore="SpeakableTextPresentCheck,UsingOnClickInXml" />

    <Button
        android:id="@+id/button5"
        android:layout_width="148dp"
        android:layout_height="126dp"
        android:layout_marginLeft="-240dp"
        android:layout_marginTop="590dp"
        android:background="@drawable/rounded_button"
        android:onClick="choice"
        android:textColor="#263238"
        android:tooltipText="advanced"
        android:translationX="15dp"
        android:translationY="40dp"
        app:backgroundTint="#5CFFFEFD"
        app:iconTint="#040000"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView"
        app:layout_constraintVertical_bias="0.675"
        tools:ignore="SpeakableTextPresentCheck,UsingOnClickInXml" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="49dp"
        android:layout_height="53dp"
        android:layout_marginLeft="-200dp"
        android:layout_marginTop="480dp"
        android:rotation="43"
        android:translationY="5dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView"
        app:layout_constraintVertical_bias="0.553"
        app:srcCompat="@drawable/beginner" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="74dp"
        android:layout_height="24dp"
        android:layout_marginStart="45dp"
        android:layout_marginTop="620dp"
        android:text="ADVANCED"
        android:textColor="#070707"
        android:textStyle="bold"
        android:translationX="7dp"
        android:translationY="40dp"
        app:layout_constraintBottom_toBottomOf="@+id/button5"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView3"
        app:layout_constraintVertical_bias="0.304" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="67dp"
        android:layout_height="60dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="480dp"
        android:translationX="30dp"
        android:translationY="5dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.188"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView"
        app:srcCompat="@drawable/inter" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="-260dp"
        android:layout_marginTop="490dp"
        android:text="BEGINNER"
        android:textColor="#090909"
        android:textStyle="bold"
        android:translationY="5dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.189"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView5"
        app:layout_constraintVertical_bias="0.0" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="56dp"
        android:layout_height="55dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="610dp"
        android:translationX="15dp"
        android:translationY="40dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.738"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView"
        app:srcCompat="@drawable/advance" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="12dp"
        android:layout_marginTop="490dp"
        android:text="INTERMEDIATE"
        android:textColor="#070707"
        android:textStyle="bold"
        android:translationX="33dp"
        android:translationY="5dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.775"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView6"
        app:layout_constraintVertical_bias="0.063" />
</LinearLayout>