<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FBFBFB"
    tools:context=".dsettings">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="409dp"
        android:layout_height="729dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0">

        <ImageView
            android:id="@+id/rec2"
            android:layout_width="416dp"
            android:layout_height="251dp"
            android:contentDescription="Tea"
            android:foreground="@drawable/pizza1"
            android:foregroundGravity="fill"
            android:src="@drawable/photo_2021_11_09_00_13_23__1_"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.19"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.0"></ImageView>

        <TextView
            android:id="@+id/textView4"
            android:layout_width="272dp"
            android:layout_height="102dp"
            android:layout_marginTop="180dp"
            android:backgroundTint="#000000"
            android:text="Settings"
            android:textColor="#FFFFFF"
            android:textSize="40sp"
            android:textStyle="bold|italic"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.255"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <Button
            android:id="@+id/button14"
            android:layout_width="358dp"
            android:layout_height="50dp"
            android:background="#00FFFFFF"
            android:backgroundTint="#00FFFFFF"
            android:foregroundTint="#0E0404"
            android:text="   Profile Settings"
            android:textAlignment="viewStart"
            android:textColor="@color/black"
            android:textColorHighlight="#F8F4F4"
            android:textSize="16sp"
            android:textStyle="bold"
            android:translationX="-20dp"
            android:translationY="50dp"
            app:backgroundTint="#C81111"
            app:icon="@drawable/ic_profile"
            app:iconGravity="start"
            app:iconSize="30dp"
            app:iconTint="#0B0A0A"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.509"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.325" />

        <Button
            android:id="@+id/button12"
            android:layout_width="358dp"
            android:layout_height="50dp"
            android:background="#00FFFFFF"
            android:backgroundTint="#00FFFFFF"
            android:text="   Help"
            android:textAlignment="viewStart"
            android:textColor="#0E0E0E"
            android:textSize="16sp"
            android:textStyle="bold"
            android:translationX="-20dp"
            android:translationY="50dp"
            app:icon="@drawable/ic_help"
            app:iconGravity="start"
            app:iconTint="#000000"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.49"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.845"
            tools:ignore="TextContrastCheck,TextContrastCheck" />

        <Button
            android:id="@+id/button16"
            android:layout_width="358dp"
            android:layout_height="50dp"
            android:background="#00FFFFFF"
            android:backgroundTint="#00FFFFFF"
            android:text="   Report Abuse"
            android:textAlignment="textStart"
            android:textColor="#040303"
            android:textSize="16sp"
            android:textStyle="bold"
            android:translationX="-20dp"
            android:translationY="50dp"
            app:icon="@drawable/ic_report"
            app:iconTint="#090909"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.509"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.627"
            tools:ignore="TextContrastCheck" />

        <Button
            android:id="@+id/button17"
            android:layout_width="358dp"
            android:layout_height="50dp"
            android:background="#00FFFFFF"
            android:backgroundTint="#00FFFFFF"
            android:onClick="contact"
            android:text="  Contact Us"
            android:textAlignment="textStart"
            android:textColor="#121111"
            android:textSize="16sp"
            android:textStyle="bold"
            android:translationX="-20dp"
            android:translationY="50dp"
            app:icon="@drawable/ic_contactus"
            app:iconGravity="start"
            app:iconSize="30dp"
            app:iconTint="#090909"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.509"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.525"
            tools:ignore="TextContrastCheck" />

        <Button
            android:id="@+id/button13"
            android:layout_width="358dp"
            android:layout_height="50dp"
            android:background="#00FFFFFF"
            android:backgroundTint="#00FFFFFF"
            android:onClick="about"
            android:text="    About Us"
            android:textAlignment="viewStart"
            android:textColor="#0E0E0E"
            android:textSize="16sp"
            android:textStyle="bold"
            android:translationX="-20dp"
            android:translationY="50dp"
            app:icon="@drawable/ic_aboutus"
            app:iconGravity="start"
            app:iconTint="#000000"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.49"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.422"
            tools:ignore="OnClick,TextContrastCheck,TextContrastCheck" />

        <Button
            android:id="@+id/button10"
            android:layout_width="358dp"
            android:layout_height="50dp"
            android:background="#00FFFFFF"
            android:backgroundTint="#00FFFFFF"
            android:onClick="privacy"
            android:text="    Privacy"
            android:textAlignment="viewStart"
            android:textColor="#0E0E0E"
            android:textSize="16sp"
            android:textStyle="bold"
            android:translationX="-20dp"
            android:translationY="50dp"
            app:icon="@drawable/ic_password"
            app:iconGravity="start"
            app:iconTint="#000000"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.509"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.737"
            tools:ignore="TextContrastCheck,TextContrastCheck" />


    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>